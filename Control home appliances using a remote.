int a,c=1;
void setup() {
  // put your setup code here, to run once:
pinMode(7,OUTPUT);
pinMode(6,INPUT);
Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
a=digitalRead(7);
Serial.print(a);
while(a==0)
{
  if(a==1)
  {
   delay(300);
  a=digitalRead(7);
     c++;
}
}
if(c%2==0)
  {
    digitalWrite(7,HIGH);
  } else
    digitalWrite(7,LOW);
  }
